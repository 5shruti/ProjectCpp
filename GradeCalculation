#include <iostream>

using namespace std;

int main ()
{
  int Mark,avg,sum=0;
  cout<<"Enter Marks obtained in 5 Subjects: ";
    for(int i=0; i<5; i++)
    {
        cin>>Mark;
        sum = sum+Mark;
    }
    avg = sum/5;
  if (avg>= 75)
    {
      cout << "\n Your grade is 'A'";
    }
  if (50 <= avg && avg > 75)
    {
      cout << "\n Your grade is 'B'";
    }
  if (35 <= avg && avg > 50)
    {
      cout << "\n Your grade is 'C'";
    }
  else
    {
      cout << "\n Your grade is 'F'";
    }
  return 0;
}
